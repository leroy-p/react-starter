{
  "name": "react-starter",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@lingui/react": "3.13.3",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-helmet": "6.1.0",
    "react-router-dom": "6.3.0",
    "react-scripts": "5.0.1",
    "styled-components": "5.3.5",
    "typescript": "4.4.2"
  },
  "devDependencies": {
    "@types/node": "16.7.13",
    "@types/react": "18.0.0",
    "@types/react-dom": "18.0.0",
    "@types/react-helmet": "6.1.5",
    "@types/styled-components": "5.1.25"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "lingui:add": "lingui add-locale",
    "lingui:extract": "yarn run env:cmd lingui extract",
    "build:lingui": "lingui compile && shx rm -f src/locales/**/*.d.ts"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
